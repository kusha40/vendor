<!DOCTYPE html>
<html lang="en" class="body-full-height">
<head>
    <!-- META SECTION -->
    <meta charset="utf-8" />
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link rel="icon" href="favicon.ico" type="image/x-icon" />
    <title>@ViewBag.Title - Indusuno</title>
    <!-- END META SECTION -->
    <!-- CSS INCLUDE -->
    @Styles.Render("~/Content/CommonCss")
    @Styles.Render("~/Content/DatatableCss")
    @Styles.Render("~/Content/DashboardCss")
    @Styles.Render("~/Content/CustomCss")
    <link href="~/Content/custom/pagesize.css" rel="stylesheet" />
    @*<link href="~/Content/plugins/DataTables/jquery.dataTables.min.css" rel="stylesheet" />
        <link href="~/Content/plugins/DataTables/Buttons-1.5.4/css/buttons.dataTables.min.css" rel="stylesheet" />*@
    @*@Styles.Render("~/Content/fontawesome")
        @Styles.Render("~/Content/ionicons")*@
    <link href="~/Content/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/Ionicons/css/ionicons.min.css" rel="stylesheet" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <!-- EOF CSS INCLUDE -->
</head>
<body>
    @RenderBody()
    <!-- ./wrapper -->
    <!-- END MESSAGE BOX-->
    <!-- START PRELOADS -->
    <audio id="audio-alert" src="~/Content/audio/alert.mp3" preload="auto"></audio>
    <audio id="audio-fail" src="~/Content/audio/fail.mp3" preload="auto"></audio>
    <!-- END PRELOADS -->
    <!-- Start Script -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/JqueryUIScripts")
    @Scripts.Render("~/bundles/CommonScripts")

    <!-- DataTables -->
    @Scripts.Render("~/bundles/DataTableScripts")
    @*@Scripts.Render("~/bundles/ChartScripts")*@
    @Scripts.Render("~/bundles/DateRangePickerScripts")
    @Scripts.Render("~/bundles/OthersUIScripts")
    @Scripts.Render("~/bundles/DashboardScripts")
    @Scripts.Render("~/bundles/CustomScripts")
    @*@RenderSection("scripts", required: false)*@
    <!-- End Script -->
</body>
</html>
